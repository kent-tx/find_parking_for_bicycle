<h1>gmap</h1>
<div id="map"></div>

  <style>
  #map{
  height: 400px;
    }
</style>
  <script>
  function initMap() { 
  var map = new google.maps.Map(document.getElementById(’map’), { 
    center: { lat: 35.681236, lng: 139.767125 }, 
    zoom: 17 
  }) 
} ;
    
  </script>
<script src="https://maps.googleapis.com/maps/api/js?key=<%=ENV['GOOGLE_MAP_KEY']%>&callback=initMap" async defer></script>